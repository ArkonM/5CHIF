<Window x:Class="Schiffe_versenken.IPDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Schiffe_versenken"
        mc:Ignorable="d"
        Title="IPDialog" Height="200" Width="200">
    <StackPanel>
        <Label x:Name="IPLabel" Content="IP-Address:"></Label>
        <TextBox x:Name="IPBox"/>
        <Button x:Name="SubmitButton" Content="Submit" Click="SubmitButton_Click"></Button>
    </StackPanel>
</Window>
